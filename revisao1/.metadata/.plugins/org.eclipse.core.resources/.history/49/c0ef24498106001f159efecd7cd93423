
public class principal {

	public static void main(String[] args) {
		
	
		
		System.out.println("7X1=" +  (7*1));
		System.out.println("7X2=" + (7*2));
		System.out.println("7X3=" + (7*3));
		System.out.println("7X4=" + (7*4));
		System.out.println("7X5=" + (7*5));
		System.out.println("7X6=" + (7*6));
		System.out.println("7X7=" + (7*7));
		System.out.println("7X8=" + (7*8));
		System.out.println("7X9=" + (7*9));
		System.out.println("7x10=" + (7*10));
		
		
		
		
		
		double pi = 3.14;
		double raio = 6;
		
		System.out.println("A area do quadrado"+ (pi*raio));
		System.out.println("O perimetro do circulo é:" + (2*pi*raio));
		
		
		double temperatura =21;
		System.out.println(" 21grais em fahrenheit é igual a" + (temperatura* 9/5+32));
		
		
		
		
		

	}

